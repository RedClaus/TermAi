{
  "name": "@termai/ui-core",
  "version": "0.0.1",
  "private": true,
  "description": "Universal Bridge + Design Tokens for TermAI (Electron/Web abstraction)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": {
      "import": "./src/styles/index.css"
    },
    "./styles/index.css": "./src/styles/index.css",
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./transport": {
      "types": "./dist/transport/index.d.ts",
      "import": "./dist/transport/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@termai/shared-types": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.2.5",
    "typescript": "~5.9.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
